{
  "name": "web",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "dev": "docusaurus start -p 53500",
    "build": "docusaurus build",
    "clear": "docusaurus clear",
    "deploy": "docusaurus deploy",
    "docusaurus": "docusaurus",
    "serve": "docusaurus serve",
    "swizzle": "docusaurus swizzle",
    "tsc:c": "tsc --noEmit",
    "prettier:c": "prettier -c './**/*.{js,jsx,ts,tsx,json}'",
    "prettier:f": "prettier -w './**/*.{js,jsx,ts,tsx,json}'",
    "markdown:c": "markdownlint-cli2 \"**/*.md\"",
    "markdown:f": "markdownlint-cli2 --fix \"**/*.md\"",
    "text:c": "textlint \"**/*.md\"",
    "text:f": "textlint --fix \"**/*.md\"",
    "text:debug": "textlint --debug \"**/*.md\"",
    "cspell": "cspell \"**\" .",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "3.4.0",
    "@docusaurus/plugin-content-docs": "^3.4.0",
    "@docusaurus/preset-classic": "3.4.0",
    "@docusaurus/remark-plugin-npm2yarn": "^3.4.0",
    "@mdx-js/react": "^3.0.0",
    "@svgr/webpack": "^5.5.0",
    "clsx": "^2.0.0",
    "file-loader": "^6.2.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.4.0",
    "@docusaurus/tsconfig": "3.4.0",
    "@docusaurus/types": "3.4.0",
    "cspell": "^8.12.1",
    "markdownlint-cli2": "^0.13.0",
    "prettier": "^3.3.3",
    "textlint": "^14.0.4",
    "textlint-rule-preset-ja-spacing": "^2.4.3",
    "textlint-rule-preset-ja-technical-writing": "^10.0.1",
    "textlint-rule-prh": "^6.0.0",
    "typescript": "~5.2.2"
  },
  "engines": {
    "node": ">=20.0",
    "npm": ">=10.0.0"
  }
}
